<h1>Listing colleges</h1>

<table>
  <tr>
    <th>Sat min</th>
    <th>Sat max</th>
    <th>Zip</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @colleges.each do |college| %>
  <tr>
    <td><%= college.sat_min %></td>
    <td><%= college.sat_max %></td>
    <td><%= college.zip %></td>
    <td><%= college.name %></td>
    <td><%= link_to 'Show', college %></td>
    <td><%= link_to 'Edit', edit_college_path(college) %></td>
    <td><%= link_to 'Destroy', college, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New College', new_college_path %>
